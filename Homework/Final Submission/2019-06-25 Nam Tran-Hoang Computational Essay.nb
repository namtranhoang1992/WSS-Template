Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rxnGeneral", "[", 
   RowBox[{"drugTime_", ",", "drugTimeAmountList_List", ",", "drugRate_", ",", 
    RowBox[{"drugOrder_:", "1"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"time", "=", "drugTime"}], ",", 
      RowBox[{"timeAmountList", "=", "drugTimeAmountList"}], ",", 

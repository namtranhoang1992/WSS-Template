Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"rxnFirstDraft2", "[", 
    RowBox[{
    "time_", ",", "drugDose_", ",", "drugInterval_", ",", "drugRate_"}], 
    "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"t", "=", "time"}], ",", 
       RowBox[{"dose", "=", "drugDose"}], ",", 
       RowBox[{"rate", "=", "drugRate"}], ",", 
       RowBox[{"interval", "=", "drugInterval"}], ",", "k"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"k", "=", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "rate"}], "*", "interval"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Piecewise", "[", 
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{"t", "<", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"dose", " ", 
            RowBox[{"Exp", "[", 
             RowBox[{
              RowBox[{"-", "rate"}], " ", "t"}], "]"}]}], ",", 
           RowBox[{"0", "\[LessEqual]", "t", "<", "interval"}]}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"dose", 
            RowBox[{"(", " ", 
             RowBox[{"k", "+", "1"}], ")"}], " ", 
            RowBox[{"Exp", "[", 
             RowBox[{
              RowBox[{"-", "rate"}], 
              RowBox[{"(", 
               RowBox[{"t", "-", "interval"}], ")"}]}], "]"}]}], ",", 
           RowBox[{"interval", "\[LessEqual]", " ", "t", "<", 
            RowBox[{"2", "interval"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"dose", " ", 
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox["k", "2"], "+", "k", "+", "1"}], ")"}], 
            RowBox[{"Exp", "[", 

      RowBox[{"order", "=", "drugOrder"}], ",", "amountList", ",", "timeList",
       ",", "funcOrder", ",", "listLength", ",", 
      "sequenceAmountBeforeEachJump"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"listLength", "=", 
      RowBox[{"Length", "[", "timeAmountList", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"amountList", "=", 
      RowBox[{"timeAmountList", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"timeList", "=", 
      RowBox[{"timeAmountList", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]",
      "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"funcOrder", "[", 
       RowBox[{"A_", ",", "t_", ",", "r_", ",", "n_"}], "]"}], ":=", 
      RowBox[{
       RowBox[{"DSolveValue", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"y", "'"}], "[", "x", "]"}], "\[Equal]", 
            RowBox[{
             RowBox[{"-", "r"}], " ", 
             SuperscriptBox[
              RowBox[{"y", "[", "x", "]"}], "n"]}]}], ",", 
           RowBox[{
            RowBox[{"y", "[", "0", "]"}], "\[Equal]", "A"}]}], "}"}], ",", 
         RowBox[{"y", "[", "x", "]"}], ",", "x"}], "]"}], "/.", 
       RowBox[{"{", 
        RowBox[{"x", "\[Rule]", " ", "t"}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sequenceAmountBeforeEachJump", "[", "1", "]"}], "=", "0"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sequenceAmountBeforeEachJump", "[", "n_", "]"}], ":=", 
      RowBox[{
       RowBox[{"sequenceAmountBeforeEachJump", "[", "n", "]"}], "=", 
       RowBox[{"funcOrder", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"sequenceAmountBeforeEachJump", "[", 
           RowBox[{"n", "-", "1"}], "]"}], "+", 
          RowBox[{"amountList", "[", 
           RowBox[{"[", 
            RowBox[{"n", "-", "1"}], "]"}], "]"}]}], ",", 
         RowBox[{
          RowBox[{"timeList", "[", 
           RowBox[{"[", "n", "]"}], "]"}], "-", 
          RowBox[{"timeList", "[", 
           RowBox[{"[", 
            RowBox[{"n", "-", "1"}], "]"}], "]"}]}], ",", "rate", ",", 
         "order"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Piecewise", "[", 
      RowBox[{"Join", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{"time", "<", 
            RowBox[{"timeList", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}], "}"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"funcOrder", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"sequenceAmountBeforeEachJump", "[", "k", "]"}], "+", 
               RowBox[{"amountList", "[", 
                RowBox[{"[", "k", "]"}], "]"}]}], ",", 
              RowBox[{"time", "-", 
               RowBox[{"timeList", "[", 
                RowBox[{"[", "k", "]"}], "]"}]}], ",", "rate", ",", "order"}],
              "]"}], ",", 
            RowBox[{
             RowBox[{"timeList", "[", 
              RowBox[{"[", "k", "]"}], "]"}], "\[LessEqual]", " ", "time", 
             "<", 
             RowBox[{"timeList", "[", 
              RowBox[{"[", 
               RowBox[{"k", "+", "1"}], "]"}], "]"}]}]}], "}"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"k", ",", "1", ",", 
            RowBox[{"listLength", "-", "1"}]}], "}"}]}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{

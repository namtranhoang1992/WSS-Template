            RowBox[{"Exp", "[", 
             RowBox[{
              RowBox[{"-", "rate"}], 
              RowBox[{"(", 
               RowBox[{"t", "-", 
                RowBox[{"3", "interval"}]}], ")"}]}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"3", "interval"}], "\[LessEqual]", " ", "t", "<", 
            RowBox[{"4", "interval"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"dose", " ", 
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox["k", "4"], "+", 
              SuperscriptBox["k", "3"], "+", 
              SuperscriptBox["k", "2"], "+", "k", "+", "1"}], ")"}], " ", 
            RowBox[{"Exp", "[", 
             RowBox[{
              RowBox[{"-", "rate"}], 
              RowBox[{"(", 
               RowBox[{"t", "-", 
                RowBox[{"4", "interval"}]}], ")"}]}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"4", "interval"}], "\[LessEqual]", " ", "t", "<", 
            RowBox[{"5", "interval"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"dose", " ", 
            RowBox[{"(", 

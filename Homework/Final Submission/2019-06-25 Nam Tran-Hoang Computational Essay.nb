Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"rxnFirst", "[", 
    RowBox[{
    "time_", ",", "drugDose_", ",", "drugInterval_", ",", "drugRate_"}], 
    "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"t", "=", "time"}], ",", 
       RowBox[{"dose", "=", "drugDose"}], ",", 
       RowBox[{"rate", "=", "drugRate"}], ",", 
       RowBox[{"interval", "=", "drugInterval"}], ",", "numPeriod", ",", 
       "k"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"k", "=", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "rate"}], "*", "interval"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"numPeriod", "=", 
